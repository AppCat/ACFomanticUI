@namespace ACUI.FomanticUI
@inherits ACContentComponentBase

<CascadingValue Value="this" IsFixed="true">
    <div class="@ClassMapper.Result"
         style="@StyleMapper.Result"
         id="@Id"
         disabled="@Disabled"
         tabindex="@($"{Tabindex ?? 0}")"
         @attributes="@Attributes">
        @ChildContent
    </div>
</CascadingValue>
